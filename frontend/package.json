    "@cypress/react": "^7.0.3",
    "start-server-and-test": "^2.0.0",
    "env-cmd": "^10.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "CI=true react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "serve": "serve -s build -l 3000",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "prepare": "cd .. && husky install frontend/.husky",
    "start:dev": "cross-env REACT_APP_ENV=development npm start",
    "start:staging": "cross-env REACT_APP_ENV=staging npm start",
    "start:prod": "cross-env REACT_APP_ENV=production npm start",
    "build:dev": "cross-env REACT_APP_ENV=development npm run build",
    "build:staging": "cross-env REACT_APP_ENV=staging npm run build",
    "build:prod": "cross-env REACT_APP_ENV=production npm run build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "start-server-and-test serve http://localhost:3000 cypress:run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:component": "node scripts/generate-component.js",
    "generate:hook": "node scripts/generate-hook.js",
    "generate:service": "node scripts/generate-service.js",
    "clean": "rm -rf build node_modules/.cache",
    "clean:all": "rm -rf build node_modules",
    "reinstall": "npm run clean:all && npm install",
    "check-deps": "npx npm-check-updates",
    "update-deps": "npx npm-check-updates -u && npm install",
    "security-audit": "npm audit --audit-level=moderate",
    "security-fix": "npm audit fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/prop-types": "warn",
      "react/no-unused-prop-types": "warn",
      "react-hooks/exhaustive-deps": "warn",
      "no-console": ["warn", { "allow": ["warn", "error"] }],
      "no-debugger": "warn",
      "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
      "prefer-const": "warn"
    },
    "overrides": [
      {
        "files": ["**/*.test.{js,jsx,ts,tsx}"],
        "rules": {
          "no-console": "off"
        }
      }
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "plugins": ["prettier-plugin-tailwindcss"]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/reportWebVitals.js",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/index.{js,jsx,ts,tsx}",
      "!src/utils/test-utils.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": ["text", "lcov", "html"],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "proxy": "http://localhost:3001",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.6.5"
  }
}